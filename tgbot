import telebot
import time
from telebot import types

bot = telebot.TeleBot('6175394098:AAEQiY3ue7sfmc1tpxrIbVrF_v3V1KCbUwU')

@bot.message_handler(commands=['start'])
def main(message):
    delay_seconds = 3
    bot.send_message(message.chat.id, f'<i>–ò–Ω–∏—Ü–∏–∏—Ä—É–µ–º —Å–≤—è–∑—å —Å –±–ª–æ–∫—á–µ–π–Ω-—Å–µ—Ç—å—é... üí†\n\n–°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∏—Ä—É–µ–º —Ç–≤–æ–π NFT-–ø–æ—Ä—Ç—Ñ–µ–ª—å —Å –ø–æ—Å–ª–µ–¥–Ω–∏–º–∏ —Ç–µ–Ω–¥–µ–Ω—Ü–∏—è–º–∏ –∫—Ä–∏–ø—Ç–æ-–º–∏—Ä–∞...</i>', parse_mode='HTML')
    time.sleep(delay_seconds)
    knopki0 = types.InlineKeyboardMarkup(row_width=1)
    knopka1 = types.InlineKeyboardButton('–ß—Ç–æ —Ç–∞–∫–æ–µ TonkaCook? ‚ÑπÔ∏è', callback_data='Question_1')
    knopka2 = types.InlineKeyboardButton('–Ø –Ω–∏—á–µ–≥–æ –Ω–µ –∑–Ω–∞—é. –ö–∞–∫ –±—ã—Ç—å? üôã‚Äç‚ôÇÔ∏è', callback_data='Question_2')
    knopka3 = types.InlineKeyboardButton('–ù–∞—à–∏ —Å–æ—Ü—Å–µ—Ç–∏ üíª', callback_data='Question_3')
    knopka4 = types.InlineKeyboardButton('–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –æ–ø–ª–∞—Ç–µ üí≥', callback_data='Question_4')
    knopka5 = types.InlineKeyboardButton('–ó–∞–ø–æ–ª–Ω–∏—Ç—å Google-—Ñ–æ—Ä–º—É üìù', callback_data='Question_5')
    knopka6 = types.InlineKeyboardButton('–ü—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –ø–æ–¥–ø–∏—Å–∫—É ‚úÖ', callback_data='Question_6')
    knopki0.add(knopka1, knopka2, knopka3, knopka4, knopka5, knopka6)
    bot.send_message(message.chat.id, f'–ü—Ä–∏–≤–µ—Ç, <b>{message.from_user.first_name}</b>! üëãüèº \n–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –∫ –Ω–∞–º –≤ –≥—Ä—É–ø–ø—É. –Ø —Ç–≤–æ–π –ø—É—Ç–µ–≤–æ–¥–∏—Ç–µ–ª—å –ø–æ <code>TonkaCook</code>. –ú–æ–∂–µ—à—å –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –º–µ–Ω—é –Ω–∏–∂–µ –∏ –∑–∞–¥–∞—Ç—å –≤–æ–ø—Ä–æ—Å—ã.', reply_markup=knopki0, parse_mode='HTML')

@bot.message_handler(commands=['menu'])
def main(message):
    delay_seconds = 3
    bot.send_message(message.chat.id, '<i>üîã –†–∞–¥ —Ç–≤–æ–µ–º—É –≤–æ–∑–≤—Ä–∞—â–µ–Ω–∏—é! TonkaBot –∞–∫—Ç–∏–≤–Ω–æ –Ω–∞—Å—Ç—Ä–∞–∏–≤–∞–µ—Ç –º–µ–Ω—é —Å –Ω—É–∂–Ω—ã–º–∏ —Å—Å—ã–ª–∫–∞–º–∏ –∏ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–µ–π. –í—Å–µ –±—É–¥–µ—Ç –≥–æ—Ç–æ–≤–æ —á–µ—Ä–µ–∑ –º–≥–Ω–æ–≤–µ–Ω–∏–µ!</i>', parse_mode='HTML')
    time.sleep(delay_seconds)
    knopki0 = types.InlineKeyboardMarkup(row_width=1)
    knopka1 = types.InlineKeyboardButton('–ß—Ç–æ —Ç–∞–∫–æ–µ TonkaCook? ‚ÑπÔ∏è', callback_data='Question_1')
    knopka2 = types.InlineKeyboardButton('–Ø –Ω–∏—á–µ–≥–æ –Ω–µ –∑–Ω–∞—é. –ö–∞–∫ –±—ã—Ç—å? üôã‚Äç‚ôÇÔ∏è', callback_data='Question_2')
    knopka3 = types.InlineKeyboardButton('–ù–∞—à–∏ —Å–æ—Ü—Å–µ—Ç–∏ üíª', callback_data='Question_3')
    knopka4 = types.InlineKeyboardButton('–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± –æ–ø–ª–∞—Ç–µ üí≥', callback_data='Question_4')
    knopka5 = types.InlineKeyboardButton('–ó–∞–ø–æ–ª–Ω–∏—Ç—å Google-—Ñ–æ—Ä–º—É üìù', callback_data='Question_5')
    knopka6 = types.InlineKeyboardButton('–ü—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –ø–æ–¥–ø–∏—Å–∫—É ‚úÖ', callback_data='Question_6')
    knopki0.add(knopka1, knopka2, knopka3, knopka4, knopka5, knopka6)
    bot.send_message(message.chat.id, f'–î–æ–±—Ä—ã–π –¥–µ–Ω—å, <b>{message.from_user.first_name}</b>! üòâ \n–í—ã–±–∏—Ä–∞–π, –∑–∞–∏–Ω—Ç–µ—Ä–µ—Å–æ–≤–∞–≤—à–∏–µ —Ç–µ–±—è –≤–æ–ø—Ä–æ—Å—ã –Ω–∏–∂–µ.', reply_markup=knopki0, parse_mode='HTML')

@bot.message_handler(func=lambda message: True)
def handle_all_messages(message):
    keyboard = types.ReplyKeyboardMarkup(resize_keyboard=True, one_time_keyboard=True)
    but1 = types.KeyboardButton(text='/menu ')
    keyboard.row(but1)
    bot.send_message(message.chat.id, "–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ–º–∞–Ω–¥—É /menu, —á—Ç–æ–±—ã –≤—ã–±—Ä–∞—Ç—å –≤–æ–∑–º–æ–∂–Ω—ã–µ –¥–µ–π—Å—Ç–≤–∏—è", reply_markup=keyboard)


@bot.callback_query_handler(func=lambda call: call.data == 'Question_1')
def Question1(callback):
    knopki1 = types.InlineKeyboardMarkup(row_width=1)
    delay_seconds = 4
    button1_question1 = types.InlineKeyboardButton(text= '–ù–∞—à–∏ —Ä–∞–∑–¥–µ–ª—ã', callback_data='units')
    button2_question2 = types.InlineKeyboardButton(text= '–ù–∞—à–∞ –∫–æ–º–∞–Ω–¥–∞', callback_data='team')
    button2_question3 = types.InlineKeyboardButton(text= '–ù–∞—à roadmap', callback_data='roadmap')
    knopki1.add(button1_question1, button2_question2, button2_question3)
    bot.send_message(callback.message.chat.id, '<i>–ì–µ–Ω–µ—Ä–∏—Ä—É—é –æ—Ç—á–µ—Ç –æ TonkaCook... ‚òÑÔ∏è\n\n–°—Ç–µ–Ω–¥–±–∞–π, —Å–∫–æ—Ä–æ —Ä–∞—Å–∫—Ä–æ—é –≤—Å–µ –¥–µ—Ç–∞–ª–∏ –Ω–∞—à–µ–π –∫—Ä–∏–ø—Ç–æ-—ç–∫–æ—Å–∏—Å—Ç–µ–º—ã.</i>', parse_mode='HTML')
    time.sleep(delay_seconds)
    bot.send_message(callback.message.chat.id, '<code>TonkaCook</code> - –ø—Ä–æ–µ–∫—Ç, —Å–ø–µ—Ü–∏–∞–ª–∏–∑—É—Ä—É—é—â–∏–π—Å—è –Ω–∞ –∞–Ω–∞–ª–∏—Ç–∏–∫–µ –≤ —Å—Ñ–µ—Ä–µ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç –∏ NFT.\n\n–ú—ã –ø—Ä–µ–¥–ª–∞–≥–∞–µ–º –ø–æ–¥—Ä–æ–±–Ω—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –ø–æ –∞–∫—Ç—É–∞–ª—å–Ω—ã–º NFT-–¥—Ä–æ–ø–∞–º –∏ —Ç–µ—Å—Ç–Ω–µ—Ç–∞–º, –∫–æ—Ç–æ—Ä—É—é —Ä–∞–∑–º–µ—â–∞–µ–º –Ω–∞ –Ω–∞—à–µ–º –∑–∞–∫—Ä—ã—Ç–æ–º Discord —Å–µ—Ä–≤–µ—Ä–µ ‚Äî –∑–¥–µ—Å—å —Å–æ—Å—Ä–µ–¥–æ—Ç–æ—á–µ–Ω–∞ –æ—Å–Ω–æ–≤–Ω–∞—è —á–∞—Å—Ç—å –Ω–∞—à–∏—Ö –¥–∞–Ω–Ω—ã—Ö –∏ –∞–Ω–∞–ª–∏–∑–æ–≤. –í—ã —Ç–∞–∫–∂–µ –Ω–∞–π–¥–µ—Ç–µ –æ—Ç–¥–µ–ª—å–Ω—ã–µ —Ä–∞–∑–¥–µ–ª—ã –∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –≤ –Ω–∞—à–µ–º Telegram-–∫–∞–Ω–∞–ª–µ, –Ω–∞–ø—Ä–∏–º–µ—Ä —Å–ø–µ–∫—É–ª—è—Ü–∏–∏ –∫—Ä–∏–ø—Ç–æ-–∞–∫—Ç–∏–≤–∞–º–∏ —Å –∫—Ä–µ–¥–∏—Ç–Ω—ã–º –ø–ª–µ—á–æ–º –∏ –±–µ–∑.\n\n–î–æ—Å—Ç—É–ø –∫–æ –≤—Å–µ–π —ç—Ç–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ—Å—É—â–µ—Å—Ç–≤–ª—è–µ—Ç—Å—è –Ω–∞ –æ—Å–Ω–æ–≤–µ –µ–∂–µ–º–µ—Å—è—á–Ω–æ–π –ø–æ–¥–ø–∏—Å–∫–∏, –ø–æ–∑–≤–æ–ª—è—è –≤–∞–º –±—ã—Ç—å –≤ –∫—É—Ä—Å–µ –ø–æ—Å–ª–µ–¥–Ω–∏—Ö —Ç—Ä–µ–Ω–¥–æ–≤ –∏ –Ω–æ–≤–æ—Å—Ç–µ–π –≤ –º–∏—Ä–µ —Ü–∏—Ñ—Ä–æ–≤—ã—Ö –∞–∫—Ç–∏–≤–æ–≤.', reply_markup=knopki1, parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'units')
def Question1_units(callback):
    knopki11 = types.InlineKeyboardMarkup(row_width=3)
    but_units1 = types.InlineKeyboardButton(text= 'NFT', callback_data='nft')
    but_units2 = types.InlineKeyboardButton(text= 'Testnets', callback_data='testnet')
    but_units3 = types.InlineKeyboardButton(text= 'Futures', callback_data='futures')
    but_units4 = types.InlineKeyboardButton(text= 'Season Money', callback_data='season_money')
    knopki11.add(but_units1, but_units2, but_units3, but_units4)
    bot.send_message(callback.message.chat.id, '<code>TonkaCook</code> –∞–∫—Ç–∏–≤–Ω–æ —Ä–∞–∑–≤–∏–≤–∞–µ—Ç—Å—è, –ø—Ä–µ–¥–ª–∞–≥–∞—è –≤–∞–º —á–µ—Ç—ã—Ä–µ –∫–ª—é—á–µ–≤—ã—Ö —Ä–∞–∑–¥–µ–ª–∞: <b>NFT</b>, <b>Testnets</b>, <b>Futures</b> –∏ <b>Season Money</b>.\n\n–ö–∞–∂–¥—ã–π –∏–∑ —ç—Ç–∏—Ö —Ä–∞–∑–¥–µ–ª–æ–≤ –∏–º–µ–µ—Ç —Å–≤–æ–∏ –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç–∏ —Å —Ç–æ—á–∫–∏ –∑—Ä–µ–Ω–∏—è –ø–æ—Ç–µ–Ω—Ü–∏–∞–ª—å–Ω–æ–π –ø—Ä–∏–±—ã–ª–∏ –∏ –≤—Ä–µ–º–µ–Ω–Ω—ã—Ö –∑–∞—Ç—Ä–∞—Ç. –ö–∞–Ω–∞–ª—ã –ø–æ–¥–æ–±—Ä–∞–Ω—ã —Ç–∞–∫–∏–º –æ–±—Ä–∞–∑–æ–º, —á—Ç–æ–±—ã –≤—ã –º–æ–≥–ª–∏ –≤–µ—Å—Ç–∏, –∫–∞–∫ –∫—Ä–∞—Ç–∫–æ—Å—Ä–æ—á–Ω—É—é —Ç–æ—Ä–≥–æ–≤–ª—é (—Ñ–ª–∏–ø–∞—Ç—å NFT), —Ç–∞–∫ –∏ –¥–æ–ª–≥–æ—Å—Ä–æ—á–Ω—É—é (—Ñ—å—é—á–µ—Ä—Å—ã, —Ç–µ—Å—Ç–Ω–µ—Ç—ã). –ú—ã –ø–ª–∞–Ω–∏—Ä—É–µ–º –¥–∞–ª—å–Ω–µ–π—à–µ–µ —Ä–∞—Å—à–∏—Ä–µ–Ω–∏–µ –∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –∫–∞–Ω–∞–ª–æ–≤, –∞ —Ç–∞–∫–∂–µ —É—Å–∏–ª–µ–Ω–∏–µ –Ω–∞—à–µ–π –∫–æ–º–∞–Ω–¥—ã. –í—Å–µ —ç—Ç–∏ –ø–ª–∞–Ω—ã –æ—Ç—Ä–∞–∂–µ–Ω—ã –≤ –Ω–∞—à–µ–π –¥–æ—Ä–æ–∂–Ω–æ–π –∫–∞—Ä—Ç–µ. üó∫Ô∏è\n\n–ß—Ç–æ–±—ã —É–∑–Ω–∞—Ç—å –±–æ–ª—å—à–µ –æ –∫–∞–∂–¥–æ–º –∏–∑ —Ç–µ–∫—É—â–∏—Ö —Ä–∞–∑–¥–µ–ª–æ–≤, –ø—Ä–æ—Å—Ç–æ –Ω–∞–∂–º–∏—Ç–µ –Ω–∞ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–∏–µ –∫–Ω–æ–ø–∫–∏ –Ω–∏–∂–µ. ‚§µ Ô∏è', reply_markup=knopki11, parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'nft')
def Question1_units_nft(callback):
    bot.send_message(callback.message.chat.id, 'üîÑ <b>NFT, Shitcoins</b> ‚Äî –ù–∞—à–∞ –∫–æ–º–∞–Ω–¥–∞ <code>TonkaCook</code> —Ç—â–∞—Ç–µ–ª—å–Ω–æ —Å–ª–µ–¥–∏—Ç –∑–∞ —Ä—ã–Ω–∫–æ–º –∏ —Ä–µ–≥—É–ª—è—Ä–Ω–æ –æ–±–Ω–æ–≤–ª—è–µ—Ç –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –ø–æ –∫–ª—é—á–µ–≤—ã–º –±–ª–æ–∫—á–µ–π–Ω–∞–º: SOL, BTC, ADA, ETH, POLYGON. –í –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä—ã–Ω–æ—á–Ω—ã—Ö —Ç—Ä–µ–Ω–¥–æ–≤, –æ–±—ä–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –ø–æ –∫–∞–∂–¥–æ–º—É –±–ª–æ–∫—á–µ–π–Ω—É –º–æ–∂–µ—Ç –≤–∞—Ä—å–∏—Ä–æ–≤–∞—Ç—å—Å—è. –≠—Ç–æ—Ç —Ä–∞–∑–¥–µ–ª –≤–µ–¥–µ—Ç—Å—è –Ω–∞—à–∏–º –æ–ø—ã—Ç–Ω—ã–º –∞–Ω–∞–ª–∏—Ç–∏–∫–æ–º smipet300. –û–Ω –∑–∞–Ω–∏–º–∞–µ—Ç—Å—è —ç—Ç–∏–º —Ç—Ä–µ—Ç–∏–π –≥–æ–¥ –ø–æ –Ω–µ—Å–∫–æ–ª—å–∫–æ —á–∞—Å–æ–≤ –≤ –¥–µ–Ω—å, –ø–æ–Ω–∏–º–∞—è –≤—Å–µ —Ç–æ–Ω–∫–æ—Å—Ç–∏ –∏ –æ—Å–æ–±–µ–Ω–Ω–æ—Å—Ç–∏ —Ä—ã–Ω–∫–∞.\n\nüß¨ <b>NFT –§–ª–∏–ø—ã –∏ –ö–æ–ª–ª—ã</b> ‚Äî –ü–æ–º–∏–º–æ —ç—Ç–æ–≥–æ, –º—ã –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–∏ –ø—É–±–ª–∏–∫—É–µ–º –∫–æ–ª–ª—ã –∏ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏ –ø–æ –ø—Ä–∏–æ–±—Ä–µ—Ç–µ–Ω–∏—é —Ä–∞–∑–ª–∏—á–Ω—ã—Ö NFT-–∫–æ–ª–ª–µ–∫—Ü–∏–π –Ω–∞ –∫—Ä–∞—Ç–∫–æ\—Å—Ä–µ–¥–Ω–µ\–¥–æ–ª–≥–æ—Å—Ä–æ—á–Ω—ã–µ –ø–µ—Ä–∏–æ–¥—ã, –¥–∞–≤–∞—è –≤–∞–º –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å –±—ã—Ç—å –≤ —á–∏—Å–ª–µ –ø–µ—Ä–≤—ã—Ö, –∫—Ç–æ —É–∑–Ω–∞–µ—Ç –æ –ø–µ—Ä—Å–ø–µ–∫—Ç–∏–≤–Ω—ã—Ö –∏–Ω–≤–µ—Å—Ç–∏—Ü–∏—è—Ö.', parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'testnet')
def Question1_units_testnet(callback):
    bot.send_message(callback.message.chat.id, 'üåê <b>Testnets</b> ‚Äî –†–∞–∑–¥–µ–ª –ø–æ—Å–≤—è—â–µ–Ω –ø—Ä–æ–µ–∫—Ç–∞–º –Ω–∞ –±–ª–æ–∫—á–µ–π–Ω–µ, –Ω–∞—Ö–æ–¥—è—â–∏–º—Å—è –≤ —Å—Ç–∞–¥–∏–∏ —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∏ –∏ —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏—è. –≠—Ç–∏ –ø—Ä–æ–µ–∫—Ç—ã —á–∞—Å—Ç–æ –ø—Ä–µ–¥–ª–∞–≥–∞—é—Ç —â–µ–¥—Ä—ã–µ –∞–∏–¥—Ä–æ–ø—ã –∑–∞ –∞–∫—Ç–∏–≤–Ω–æ–µ —É—á–∞—Å—Ç–∏–µ, –∫–æ—Ç–æ—Ä—ã–µ –º–æ–≥—É—Ç –¥–æ—Å—Ç–∏–≥–∞—Ç—å —Ç—ã—Å—è—á–∏ –ø—Ä–æ—Ü–µ–Ω—Ç–æ–≤. –ó–∞ –≥–∞–π–¥—ã –∏ –∞–Ω–∞–ª–∏—Ç–∏–∫—É –∑–¥–µ—Å—å –æ—Ç–≤–µ—á–∞–µ—Ç elai. –≠—Ç–æ—Ç –∫–æ–ª–ª–µ—Ä - –æ–¥–∏–Ω –∏–∑ –Ω–µ–º–Ω–æ–≥–∏—Ö, –∫—Ç–æ –∞–∫—Ç–∏–≤–Ω–æ —É—á–∞—Å—Ç–≤–æ–≤–∞–ª –≤–æ –≤—Ç–æ—Ä–æ–º —Å–µ–∑–æ–Ω–µ HyperSpace, —á—Ç–æ –ø–æ–∑–≤–æ–ª–∏–ª–æ –µ–º—É –∑–∞–Ω—è—Ç—å 45 –º–µ—Å—Ç–æ –∏ –ø–æ–ª—É—á–∏—Ç—å <tg-spoiler>5-—Ç–∏ –∑–Ω–∞—á–Ω—É—é —Å—É–º–º—É $.</tg-spoiler> –ü–æ–¥—Ä–æ–±–Ω–∞—è –∞–Ω–∞–ª–∏—Ç–∏–∫–∞ –∏ –≥–∞–π–¥—ã –Ω–∞–ø–∏—Å–∞–Ω—ã –ø–æ —Ç–æ–ø–æ–≤—ã–º –ø—Ä–æ–µ–∫—Ç–∞–º - LayerZero, KiloEx, ZKsync –∏ –¥—Ä—É–≥–∏–µ.', parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'futures')
def Question1_units_futures(callback):
