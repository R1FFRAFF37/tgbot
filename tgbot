@bot.callback_query_handler(func=lambda call: call.data == 'Question_3')
def Question3(callback):
    delay_seconds = 3
    knopki3 = types.InlineKeyboardMarkup(row_width=1)
    button1_question3 = types.InlineKeyboardButton(text= 'Telegram', url='https://t.me/TonkaCook')
    button2_question3 = types.InlineKeyboardButton(text= 'Vkontakte', url='https://vk.com/tonkacook')
    button3_question3 = types.InlineKeyboardButton(text= 'Instagram', url='https://www.instagram.com/tonka_cook?igsh=ZGNjOWZkYTE3MQ==')
    knopki3.add(button1_question3, button2_question3, button3_question3)
    bot.send_message(callback.message.chat.id, 'üîóüì≤ –¢—Ä–∞–Ω—Å–ª–∏—Ä—É—é –∫—Ä–∏–ø—Ç–æ-—Å–∏–≥–Ω–∞–ª—ã! \nTonkaBot –≥–æ—Ç–æ–≤ –Ω–∞–ø—Ä–∞–≤–∏—Ç—å –≤–∞—Å –≤ –º–∏—Ä –Ω–∞—à–∏—Ö —Å–æ—Ü–∏–∞–ª—å–Ω—ã—Ö —Å–µ—Ç–µ–π. –ü—Ä–æ—Å—Ç–æ –Ω–∞–∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫—É –≤—ã–±—Ä–∞–Ω–Ω–æ–π –ø–ª–∞—Ç—Ñ–æ—Ä–º—ã, –∏ —è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É—é –≤–∞—Å –ø—Ä—è–º–æ —Ç—É–¥–∞ ‚Äî –±–µ–∑ –±–ª–æ–∫—á–µ–π–Ω-–∑–∞–¥–µ—Ä–∂–µ–∫! –°–ª–µ–¥–∏—Ç–µ –∑–∞ –Ω–∞–º–∏, —á—Ç–æ–±—ã –±—ã—Ç—å –≤ –∫—É—Ä—Å–µ –ø–æ—Å–ª–µ–¥–Ω–∏—Ö NFT-–¥—Ä–æ–ø–æ–≤, –∫—Ä–∏–ø—Ç–æ-—Å–æ–±—ã—Ç–∏–π –∏ —Ç–µ—Å—Ç–Ω–µ—Ç–æ–≤. ')
    time.sleep(delay_seconds)
    bot.send_message(callback.message.chat.id, '<i>–í–º–µ—Å—Ç–µ –º—ã —Å–æ–∑–¥–∞—ë–º –Ω–µ—á—Ç–æ –±–æ–ª—å—à–µ–µ, —á–µ–º –ø—Ä–æ—Å—Ç–æ –∫–æ–¥ ‚Äî –Ω–∞—Å—Ç–æ—è—â–µ–µ –∫—Ä–∏–ø—Ç–æ-—Å–æ–æ–±—â–µ—Å—Ç–≤–æ! üåê‚ú®</i>' ,reply_markup=knopki3, parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'Question_5')
def Question5(callback):
    delay_seconds = 5
    knopki5 = types.InlineKeyboardMarkup(row_width=1)
    button1_question5 = types.InlineKeyboardButton(text='Google-—Ñ–æ—Ä–º–∞', url='https://forms.gle/U6MGhc5PJAPYfoDt6')
    button2_question5 = types.InlineKeyboardButton(text='–Ø –∑–∞–ø–æ–ª–Ω–∏–ª Google-—Ñ–æ—Ä–º—É', callback_data='form_completed')
    knopki5.add(button1_question5, button2_question5)
    bot.send_message(callback.message.chat.id, '–î–ª—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –æ–ø—Ç–∏–º–∞–ª—å–Ω–æ–≥–æ –≤–µ–∫—Ç–æ—Ä–∞ —Ä–∞–∑–≤–∏—Ç–∏—è, TonkaBot –Ω–∞—á–∏–Ω–∞–µ—Ç –ø—Ä–æ—Ü–µ–¥—É—Ä—É —Å–±–æ—Ä–∞ –¥–∞–Ω–Ω—ã—Ö. üìÅ\n\n–°–æ–±–∏—Ä–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –ø—Ä–µ–¥–ø–æ—á—Ç–µ–Ω–∏—è—Ö –Ω–∞—à–∏—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π, –º—ã —Ç–æ—á–Ω–µ–µ –æ–ø—Ä–µ–¥–µ–ª—è–µ–º –∫—É—Ä—Å –Ω–∞ –º–∞—Å—à—Ç–∞–±–∏—Ä–æ–≤–∞–Ω–∏–µ ‚Äî –æ—Ç –æ—Ç–∫—Ä—ã—Ç–∏—è –Ω–æ–≤—ã—Ö —Ä–∞–∑–¥–µ–ª–æ–≤ –¥–æ —Ä–∞—Å—à–∏—Ä–µ–Ω–∏—è –∫–æ–º–∞–Ω–¥—ã —Ä–∞–∑—Ä–∞–±–æ—Ç—á–∏–∫–æ–≤.\n\n–û—Å–Ω–æ–≤–æ–ø–æ–ª–∞–≥–∞—é—â–∏–π –ø—Ä–∏–Ω—Ü–∏–ø <code>TonkaCook</code> ‚Äî –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å –∏ –æ—Ç–∫—Ä—ã—Ç–æ—Å—Ç—å. –ú—ã —Ü–µ–Ω–∏–º —á–µ—Å—Ç–Ω–æ—Å—Ç—å –Ω–∞—à–∏—Ö –º–µ–º–±–µ—Ä–æ–≤ –∏ –æ–∂–∏–¥–∞–µ–º –µ—ë –≤ –≤–∞—à–∏—Ö –æ—Ç–≤–µ—Ç–∞—Ö. –ü—Ä–æ–µ–∫—Ç –Ω–µ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ—Ç —Ñ–∏–ª—å—Ç—Ä—ã –Ω–∞ –æ—Å–Ω–æ–≤–µ —ç—Ç–∏—Ö –¥–∞–Ω–Ω—ã—Ö: –Ω–∏ –≤–∞—à –≤–æ–∑—Ä–∞—Å—Ç, –Ω–∏ –æ–ø—ã—Ç –≤ –∫—Ä–∏–ø—Ç–æ–≤–∞–ª—é—Ç–Ω–æ–π —Å—Ñ–µ—Ä–µ –Ω–µ —Å—Ç–∞–Ω—É—Ç –ø—Ä–µ–≥—Ä–∞–¥–æ–π. –ö–æ–º–∞–Ω–¥–∞ —Å—Ç—Ä–µ–º–∏—Ç—Å—è –ø–æ–Ω—è—Ç—å –∫–∞–∂–¥–æ–≥–æ —É—á–∞—Å—Ç–Ω–∏–∫–∞, –ø–æ—ç—Ç–æ–º—É —Ü–µ–Ω–∏—Ç –ø–æ–ª–Ω—ã–µ –∏ –ø–æ–¥—Ä–æ–±–Ω—ã–µ –æ—Ç–≤–µ—Ç—ã, –∫–æ—Ç–æ—Ä—ã–µ —Ä–∞—Å–∫—Ä—ã–≤–∞—é—Ç –≤–∞—à–µ –≤–∏–¥–µ–Ω–∏–µ –∏ –º—ã—Å–ª–∏. –ò–º–µ–Ω–Ω–æ –ø–æ—ç—Ç–æ–º—É –º—ã –ø—Ä–æ—Å–∏–º –∏–∑–±–µ–≥–∞—Ç—å –∫—Ä–∞—Ç–∫–∏—Ö, –æ–¥–Ω–æ—Å–ª–æ–∂–Ω—ã—Ö —Ä–µ–ø–ª–∏–∫. \n–ü–æ—Å–ª–µ –∑–∞–ø–æ–ª–Ω–µ–Ω–∏—è —Ñ–æ—Ä–º—ã –Ω—É–∂–Ω–æ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –Ω–∞–∂–∞—Ç—å –Ω–∞ –∫–Ω–æ–ø–∫—É —Å –≥–∞–ª–æ—á–∫–æ–π, —á—Ç–æ–±—ã –º—ã –ø–æ–ª—É—á–∏–ª–∏ –≤–∞—à–∏ –¥–∞–Ω–Ω—ã–µ.', parse_mode='HTML')
    time.sleep(delay_seconds)
    bot.send_message(callback.message.chat.id, '<i>–í–∞—à–µ –º–Ω–µ–Ω–∏–µ —É—Å–∏–ª–∏–≤–∞–µ—Ç –Ω–∞—à—É —Å–µ—Ç—å, –∞ –∫–∞–∂–¥–æ–µ —Å–ª–æ–≤–æ –ø—Ä–æ–≥—Ä–∞–º–º–∏—Ä—É–µ—Ç –Ω–∞—à –ø—É—Ç—å –≤–ø–µ—Ä–µ–¥! üöÄ</i>', reply_markup=knopki5, parse_mode='HTML')

@bot.callback_query_handler(func=lambda call: call.data == 'form_completed')
def Question5_Googleform(callback):
    delay_seconds = 3
    time.sleep(delay_seconds)
    bot.send_message(callback.message.chat.id, 'üõ∞ –°–∏–≥–Ω–∞–ª –æ –∑–∞–ø–æ–ª–Ω–µ–Ω–∏–∏ —Ñ–æ—Ä–º—ã —É—Å–ø–µ—à–Ω–æ –ø—Ä–∏–Ω—è—Ç TonkaBot–æ–º. –ó–∞—è–≤–∫–∞ –±—É–¥–µ—Ç —Ä–∞—Å—Å–º–æ—Ç—Ä–µ–Ω–∞ –≤ —Ç–µ—á–µ–Ω–∏–µ 24-—Ö —á–∞—Å–æ–≤. –°–ø–∞—Å–∏–±–æ –∑–∞ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–Ω—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –∏ –≤–∞—à–µ –≤—Ä–µ–º—è ‚Äî –ø—Ä–æ–¥–æ–ª–∂–∞–π—Ç–µ —Å–ª–µ–¥–∏—Ç—å –∑–∞ –Ω–∞—à–∏–º–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è–º–∏! ')




bot.polling(none_stop=True)
